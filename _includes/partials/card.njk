<div class="card">
    <a class="card__link" href="{{product.url}}">
    <div class="card__head">
  <img
    class="card__image"
    src="/images/{{product.data.image}}.jpg"
    alt="{{product.data.alt}}"
  />
  </div>
  </a>
  <div class="card__label">
    <div class="card__text">{{product.data.name}}</div>
    <div class="card__price">{{product.data.price}}</div>
    <div class="card__button">
      {% include 'partials/window-small.njk' %}
    </div>
  </div>
  
</div>